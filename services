})
</script>
<script>
const offsetLog1 = document.querySelector('.case-circle__wrapperx');
const offsetLog11 = offsetLog1.offsetTop;
console.log(offsetLog11);

window.onload = function () {
const offsetLog2 = document.querySelector('.case-circle__wrapperx');
const offsetLog22 = offsetLog1.offsetTop;
console.log(offsetLog22);

	let progressCircle = document.querySelector('.progress');
	let radius = progressCircle.r.baseVal.value;
	let circumference = radius * 2 * Math.PI;
	progressCircle.style.strokeDasharray = circumference;
	let blurCircle = document.querySelector('.blur');
	blurCircle.style.strokeDasharray = circumference;

	const stickyWrapper = document.querySelector('.case-circle__wrapperx');

	const progressCircleHeadingFirst = document.querySelector(
		'.caseinnercirclecontentfirst',
	);
	const progressCircleHeadingSecond = document.querySelector(
		'.caseinnercirclecontentsecond',
	);
	const progressCircleHeadingThird = document.querySelector(
		'.caseinnercirclecontentthird',
	);
	const progressCircleHeadingFourth = document.querySelector(
		'.caseinnercirclecontentfourth',
	);
	const progressCircleHeadingFifth = document.querySelector(
		'.caseinnercirclecontentfifth',
	);
	const progressCircleHeadingSixth = document.querySelector(
		'.caseinnercirclecontentsixth',
	);

	window.addEventListener('scroll', () => {
		setProgress(setPercentage());
	});

	const setPercentage = () => {
		function getPercentage() {
			const viewportHeight = window.innerHeight;
			const scrollTop = window.scrollY;
			const elementOffsetTop = stickyWrapper.getBoundingClientRect().top + document.documentElement.scrollTop + 1150;
			const elementHeight = stickyWrapper.offsetHeight;

			if (elementOffsetTop > scrollTop + viewportHeight){
      console.log("TRUE 1");
				return 0;
			} else if (elementOffsetTop + elementHeight < scrollTop) {
      console.log("TRUE 2");
				return 100;
			} else {
      console.log("TRUE 3");
				let distance = scrollTop + viewportHeight - elementOffsetTop;
				let percentage = distance / ((viewportHeight + elementHeight) / 100);
				let percentageY = Math.round(percentage) * 1.95;
        if (percentageY > 100) {
        	return 100;
        } else {
        return percentageY;
        }
			}
		};

		if (getPercentage() == 0) { 
    	progressCircleHeadingFirst.style.opacity = '0';
			progressCircleHeadingSecond.style.opacity = '0';
			progressCircleHeadingThird.style.opacity = '0';
			progressCircleHeadingFourth.style.opacity = '0';
			progressCircleHeadingFifth.style.opacity = '0';
			progressCircleHeadingSixth.style.opacity = '0';
    } else if (getPercentage() <= 20) {
			progressCircleHeadingFirst.style.opacity = '1';
			progressCircleHeadingSecond.style.opacity = '0';
			progressCircleHeadingThird.style.opacity = '0';
			progressCircleHeadingFourth.style.opacity = '0';
			progressCircleHeadingFifth.style.opacity = '0';
			progressCircleHeadingSixth.style.opacity = '0';
		} else if (getPercentage() >= 20 && getPercentage() <= 40) {
			progressCircleHeadingFirst.style.opacity = '0';
			progressCircleHeadingSecond.style.opacity = '1';
			progressCircleHeadingThird.style.opacity = '0';
			progressCircleHeadingFourth.style.opacity = '0';
			progressCircleHeadingFifth.style.opacity = '0';
			progressCircleHeadingSixth.style.opacity = '0';
		} else if (getPercentage() >= 40 && getPercentage() <= 60) {
			progressCircleHeadingFirst.style.opacity = '0';
			progressCircleHeadingSecond.style.opacity = '0';
			progressCircleHeadingThird.style.opacity = '1';
			progressCircleHeadingFourth.style.opacity = '0';
			progressCircleHeadingFifth.style.opacity = '0';
			progressCircleHeadingSixth.style.opacity = '0';
		} else if (getPercentage() >= 60 && getPercentage() <= 80) {
			progressCircleHeadingFirst.style.opacity = '0';
			progressCircleHeadingSecond.style.opacity = '0';
			progressCircleHeadingThird.style.opacity = '0';
			progressCircleHeadingFourth.style.opacity = '1';
			progressCircleHeadingFifth.style.opacity = '0';
			progressCircleHeadingSixth.style.opacity = '0';
		} else if (getPercentage() >= 80 && getPercentage() <= 99.999) {
			progressCircleHeadingFirst.style.opacity = '0';
			progressCircleHeadingSecond.style.opacity = '0';
			progressCircleHeadingThird.style.opacity = '0';
			progressCircleHeadingFourth.style.opacity = '0';
			progressCircleHeadingFifth.style.opacity = '1';
			progressCircleHeadingSixth.style.opacity = '0';
		} else {
			progressCircleHeadingFirst.style.opacity = '0';
			progressCircleHeadingSecond.style.opacity = '0';
			progressCircleHeadingThird.style.opacity = '0';
			progressCircleHeadingFourth.style.opacity = '0';
			progressCircleHeadingFifth.style.opacity = '0';
			progressCircleHeadingSixth.style.opacity = '1';
		}
    
		return getPercentage();
	};
  
	function setProgress(percent) {
		progressCircle.style.strokeDashoffset =
			circumference - (percent / 100) * circumference;
		blurCircle.style.strokeDashoffset =
			circumference - (percent / 100) * circumference;
	}
};


</script></body>
