<script>
  var categoryButtons = document.querySelectorAll(".div-block-35 .button");
  categoryButtons.forEach(function(el) {
    el.addEventListener("click", function(e) {
      e.target.classList.toggle("active");
    });
  })

  function gradientMove() {
    var textSpans = document.querySelectorAll(".text-span, .heading-2, .heading-3");
    console.log(textSpans);

    for (var i = 0; i < textSpans.length; i++) {
      var windowHeight = window.innerHeight;
      var elementTop = textSpans[i].getBoundingClientRect().top;
      var elementVisible = 0;

      if (elementTop < 0) {
        textSpans[i].style.backgroundPosition = "100%";
      } else if (elementTop > (windowHeight - elementVisible)) {
        textSpans[i].style.backgroundPosition = "0%";
      } else {
        textSpans[i].style.backgroundPosition = 100 - (elementTop / (windowHeight - elementVisible)) * 100 + "%"
      }
    }
  }

  gradientMove();
  window.addEventListener("scroll", gradientMove);
</script>

