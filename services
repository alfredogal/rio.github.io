<script>
  	const navibar = document.querySelector('.navbar');
  	const navigationBackground = document.querySelector(".navbar-background");
  	const hamburgerButton = document.querySelector(".menu-button-2");
  	let fh = window.innerHeight * 0.01;
  
  	document.documentElement.style.setProperty('--fh', `${fh}px`);
  
	window.addEventListener('scroll', function() {
      fh = window.innerHeight * 0.01;
      document.documentElement.style.setProperty('--fh', `${fh}px`);
      navibar.classList.toggle('scrolled', window.scrollY > 0);
    })
    
    hamburgerButton.addEventListener('click', function() {
      	if (!hamburgerButton.classList.contains('w--open')) {
    		navigationBackground.style.backgroundColor = "#000";
        } else {
          navigationBackground.style.backgroundColor = null;
        }
    });
</script>
